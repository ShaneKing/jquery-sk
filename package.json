{
  "name": "sk-js",
  "version": "0.5.4",
  "description": "ShaneKing for JavaScript",
  "homepage": "https://github.com/ShaneKing/sk-js",
  "author": {
    "name": "ShaneKing",
    "email": "github@azolla.org",
    "url": "http://shaneking.org"
  },
  "bugs": {
    "url": "https://github.com/ShaneKing/sk-js/issues"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/Apache-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ShaneKing/sk-js.git"
  },
  "files": [
    "dist",
    "lib",
    "public",
    ".editorconfig",
    "LICENSE",
    "README.md",
    "SK_for_WebStorm_Code_Style.xml"
  ],
  "keywords": [
    "ShaneKing",
    "SK"
  ],
  "main": "lib/index.js",
  "module": "dist/index.esm.js",
  "jest": {
    "collectCoverage": true
  },
  "scripts": {
    "build": "npm run build:dist && npm run build:lib && npm run doc:build",
    "build:dist": "umi-lib build",
    "build:lib": "umi-tools build",
    "doc:build": "umi-lib doc build",
    "doc:deploy": "umi-lib doc deploy",
    "doc:dev": "umi-lib doc dev",
    "prepublishOnly": "npm run build",
    "test": "umi test && codecov"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "jquery": "3.4.1",
    "js-cookie": "2.2.0",
    "js-md5": "0.7.3",
    "lodash": "4.17.14",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "codecov": "3.5.0",
    "jsdom": "15.1.1",
    "umi": "2.8.11",
    "umi-library": "1.6.0",
    "umi-tools": "0.4.0"
  }
}

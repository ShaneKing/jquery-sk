{
  "name": "sk-js",
  "version": "0.5.11",
  "description": "ShaneKing for JavaScript",
  "homepage": "https://github.com/ShaneKing/sk-js",
  "author": {
    "name": "ShaneKing",
    "email": "github@azolla.org",
    "url": "http://shaneking.org"
  },
  "bugs": {
    "url": "https://github.com/ShaneKing/sk-js/issues"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/Apache-2.0"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ShaneKing/sk-js.git"
  },
  "files": [
    "dist",
    "lib",
    "public",
    ".editorconfig",
    "LICENSE",
    "README.md",
    "SK_for_WebStorm_Code_Style.xml"
  ],
  "keywords": [
    "ShaneKing",
    "SK"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run js:dist && npm run js:lib && npm run doc:build",
    "js:dist": "webpack",
    "js:lib": "rm -rf lib && babel src -d lib",
    "doc:build": "docz build",
    "doc:deploy": "docz deploy",
    "doc:dev": "docz dev",
    "lint": "eslint --fix --ext .js src",
    "prepublishOnly": "npm run build",
    "test": "father test --coverage && codecov"
  },
  "dependencies": {
    "crypto-js": "3.1.9-1",
    "jquery": "3.4.1",
    "js-cookie": "2.2.0",
    "js-md5": "0.7.3",
    "lodash": "4.17.15",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/preset-env": "7.5.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "codecov": "3.5.0",
    "eslint": "6.1.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "jsdom": "15.1.1",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6",
    "docz": "1.2.0",
    "typescript": "3.5.3",
    "dotenv": "8.0.0",
    "docz-theme-default": "1.2.0"
  },
  "jest": {
    "collectCoverage": true
  }
}
